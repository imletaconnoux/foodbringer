<%= form_for @address do |f| %>
<%= f.label :label %>:
<%= f.text_field :label %><br>
<%= f.label :street_address %>:
<%= f.text_field :street_address %><br>
<%= f.label :city %>:
<%= f.text_field :city %><br>
<%= f.label :state %>:
<%= f.text_field :state %><br>
<%= f.label :zip %>:
<%= f.text_field :zip %><br>
<%= f.submit 'Submit' %>
<% end %>
